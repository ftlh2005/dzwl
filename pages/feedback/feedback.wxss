.container{
  background-color: #F6F6F6;
  box-sizing: border-box;
  width: 100vw;
  height: 100vh;
}
.input-box{
  width: 100%;
  height: 280rpx;
  background-color: #FFF;
  box-sizing: border-box;
  padding: 30rpx;
  margin-top: 2rpx;
}
textarea{
  width: 100%;
  height: 220rpx;
  color: #333;
  font-size: 26rpx;
}
.contact{
  box-sizing: border-box;
  padding: 27rpx 30rpx;
  margin-top:20rpx;
  background-color: #FFF;
  width: 100%;
  height: 80rpx;
  color: #333;
  font-size: 26rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.contact>input{
  font-size: 26rpx;
  width: 500rpx;
}
.submit{
  position: fixed;
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  bottom: 80rpx;
  background: #0A6EDC;
  color: #FFF;
  left: 0;
  right: 0;
  font-size: 26rpx;
}
