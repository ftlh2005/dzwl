.container{
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
  padding: 0;
  background-color: #F6F6F6;
}
.funtion{
  width: 100%;
  height: 80rpx;
  background-color: #FFF;
  margin-top:2rpx;
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #333;
}
.icon{
  width: 40rpx;
  height: 40rpx;
}
.tit{
  font-size: 26rpx;
  margin-left:20rpx;
}
.content{
  width: 480rpx;
  margin-left: 10rpx;
  font-size: 26rpx;
}
.upload{
	/* position:absolute; */
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  color:#FF5959;
  font-size: 24rpx;
  border:1rpx solid #FF5959;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0 8rpx;
	/* right: 30rpx; */
}
.submit{
  color: #FFF;
  background-color: #0A6EDC;
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  bottom: 80rpx;
  position: fixed;
  left:0;
  right: 0;
  font-size: 26rpx;
}
