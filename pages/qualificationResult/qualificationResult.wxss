.container{
  width: 100%;
  height: 100vh;
  background-color: #F6F6F6;
}
.feed-info{
  margin-top: 1rpx;
  background-color: #FFF;
  width: 100%;
  height: 50vh;
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}
.feed-info>image{
  width: 100rpx;
}
.feed-info>text{
  color: #333;
  font-size: 26rpx;
  margin-top: -80rpx;
  width: 85vw;
}
.feed-info .notice{
  width: 100%;
  line-height: 50rpx;
}
.notice0{
  font-size: 26rpx;
	color: #ff5656;
}
.notice1{
  font-size: 24rpx;
  color: #999999;
}
.editInfo{
  color: #FFF;
  background-color: #0A6EDC;
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  bottom: 80rpx;
  position: fixed;
  left:0;
  right: 0;
  font-size: 26rpx;
}